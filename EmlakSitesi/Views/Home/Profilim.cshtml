@model EmlakSitesi.Models.Anasayfa.AnasayfaViewModel
@{
    ViewBag.Title = "Profilim";
    Layout = "~/Views/Shared/_LoginLayout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <title>Bootstrap Example</title>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#"></a>
            </div>
            <ul class="nav navbar-nav">
                <li><a href="#">İlan Ver</a></li>
                <li><a href="#">İlanlarım</a></li>
                <li><a href="#">Favorilerim</a></li>
                <li><a href="#">Mesajlarım</a></li>
                <li><a href="#">Profilim</a></li>
            </ul>
        </div>
    </nav>

    <div class="w3-container">
        <table class="w3-table-all w3-centered">
            <tr>
                <th><img src="https://img.icons8.com/wired/64/000000/multi-edit.png" /></th>
                <th><img src="https://img.icons8.com/nolan/64/new-message.png" /></th>
                <th><img src="https://img.icons8.com/dusk/64/000000/user.png"/></th>
            </tr>
            <tr>
                <td>İlanlarım<br />@Html.ActionLink("İlan Ver", "Index", "Home")</td>
